echo "Setting up Purroser..."
rm -rf purr
rm -rf build
python3 -m venv purr
source purr/bin/activate
pip install --upgrade pip
pip install PyQt5
pip install PyQtWebEngine
pip install setuptools
pip install Cython
deactivate
chmod +x Thoqfile
echo "Done! Now run ./Thoqfile to build Purroser."
echo "Run python purrooser.py to run Purroser."